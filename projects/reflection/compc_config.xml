<?xml version="1.0"?>
<flex-config xmlns="http://www.adobe.com/2006/flex-config">
	<output>bin/nexuslib.reflection.swc</output>
	<use-network>true</use-network>
	<target-player>11.0</target-player>
	<warnings>true</warnings>
	<benchmark>false</benchmark>
	<compiler>
		<accessible>false</accessible>
		<allow-source-path-overlap>false</allow-source-path-overlap>
		<optimize>true</optimize>
		<strict>true</strict>
		<es>false</es>
		<show-actionscript-warnings>true</show-actionscript-warnings>
		<show-binding-warnings>true</show-binding-warnings>
		<show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>
		<use-resource-bundle-metadata>true</use-resource-bundle-metadata>
		<verbose-stacktraces>false</verbose-stacktraces>
		<source-path>
			<path-element>src</path-element>
		</source-path>
		<library-path>
			<path-element>../../lib/blooddy_crypto_0.3.5/blooddy_crypto.swc</path-element>
		</library-path>
	</compiler>
	<include-classes>
		<class>avmplus.AVMDescribeType</class>
		<class>nexus.nexuslib_internal</class>
		<class>nexus.errors.ClassNotFoundError</class>
		<class>nexus.errors.NotImplementedError</class>
		<class>nexus.utils.ObjectUtils</class>
		<class>nexus.utils.Parse</class>
		<class>nexus.utils.reflection.AbstractFieldInfo</class>
		<class>nexus.utils.reflection.AbstractMemberInfo</class>
		<class>nexus.utils.reflection.AbstractMetadataRecipient</class>
		<class>nexus.utils.reflection.FieldInfo</class>
		<class>nexus.utils.reflection.ITypeInfoCreator</class>
		<class>nexus.utils.reflection.MetadataInfo</class>
		<class>nexus.utils.reflection.MethodInfo</class>
		<class>nexus.utils.reflection.MethodParameterInfo</class>
		<class>nexus.utils.reflection.PropertyInfo</class>
		<class>nexus.utils.reflection.Reflection</class>
		<class>nexus.utils.reflection.TypeInfo</class>
		<class>nexus.utils.reflection.TypeInfoCreatorJson</class>
		<class>nexus.utils.reflection.TypeInfoCreatorXml</class>
		<class>nexus.utils.serialization.ISerializer</class>
		<class>nexus.utils.serialization.amf.AmfSerializer</class>
		<class>nexus.utils.serialization.json.IJsonSerializable</class>
		<class>nexus.utils.serialization.json.JsonParser</class>
		<class>nexus.utils.serialization.json.JsonParserBlooddy</class>
		<class>nexus.utils.serialization.json.JsonSerializer</class>
		<class>nexus.utils.serialization.xml.IXmlSerializable</class>
		<class>nexus.utils.serialization.xml.XmlMetadata</class>
		<class>nexus.utils.serialization.xml.XmlSerializer</class>
	</include-classes>
</flex-config>